{
  "name": "vite-plugin-update-checker",
  "version": "1.0.0",
  "description": "A vite plugin to check for  updates of published html files.",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --dts --minify --format cjs,esm",
    "test": " vite build -c ./demo/vite.config.ts"
  },
  "keywords": [],
  "author": "3000-codes",
  "license": "MIT",
  "devDependencies": {
    "tsup": "^8.0.1",
    "typescript": "^5.2.2",
    "vite": "^5.0.6"
  },
  "dependencies": {
    "@types/node": "^20.10.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/3000-codes/vite-plugin-update-checker"
  }
}
